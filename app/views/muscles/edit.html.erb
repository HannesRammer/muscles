<h1>Bearbeiten muscle</h1>

<%= form_for(@muscle) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :picture_id %>
    <%= f.text_field :picture_id %>
    <%= f.label :name %>
    <%= f.text_area :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to t('view'), @muscle %> |
<%= link_to t('back'), muscles_path %>