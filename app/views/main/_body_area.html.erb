<map name="body_area" id="body_area">
  <%= render "layouts/body_vars" %>
  <%@body_vars.each do |vars|%>
      <area shape="<%=vars[0]%>" coords="<%=vars[1]%>" href="#"
            onMouseover="hover('<%=vars[2]%>');"

            onMouseout="hover('');" onclick="document.querySelector('#muscle_selected').value = '<%=j vars[2] %>';$('#submit_search_string').click();/*selected('<%=main_muscle_url(vars[2])%>');*/" alt="<%=vars[2]%>"/>
  <%end%>
</map>